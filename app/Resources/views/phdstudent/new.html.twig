{% extends 'base.html.twig' %}

{% block body %}

    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading"><h1>PhD student</h1></div>
            <div class="panel-body">
            </div>
        </div>
        <div>
            <a class="btn btn-primary" href="{{ path('phdstudent_index') }}">Back to the List</a>
        </div>
    </div>
    <div class="col-md-8">
        <div class="panel panel-default">
            <div class="panel-heading"><h1>Create a PhD student </h1></div>
            <div class="panel-body">
                <fieldset>
                    <legend>Add</legend>
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <div class="btngroup">
                        <input class="btn btn-primary" type="submit" value="Create" />
                    </div>
                    {{ form_end(form) }}
                </fieldset>
            </div>
        </div>
    </div>

{% endblock %}