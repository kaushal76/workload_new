{% extends 'base.html.twig' %}

{% block body %}
    <h1>Preparationcategory</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ preparationCategory.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ preparationCategory.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ preparationCategory.code }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('preparationcategory_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('preparationcategory_edit', { 'id': preparationCategory.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
